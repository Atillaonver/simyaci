<FilesMatch "\.(ttf|ttc|otf|eot|woff|woff2|font.css|css|js)$">
    Header set Access-Control-Allow-Origin "*"
</FilesMatch>

<IfModule mod_headers.c>
   Header set Referrer-Policy "no-referrer"
   Header set Access-Control-Allow-Origin "*"
   # Eğer hala sorun yaşıyorsanız geçici olarak aşağıdaki satırı yorum satırı yapın (#)
   Header always set X-Content-Type-Options "nosniff"
</IfModule>

Options +FollowSymlinks
Options -Indexes

RewriteEngine On
RewriteBase /

# CSS ve JS kurallarını daha kapsayıcı hale getirdik
RewriteRule ^(?:.*/)?bytao-([a-zA-Z0-9]+)-([0-9]+)-([0-9]+)\.css$ css.php?val1=$1&val2=$2&val3=$3 [L,QSA]
# RewriteRule ^(?:.*/)?bytao-([a-zA-Z0-9]+)-([0-9]+)-([0-9]+)\.js$ script.php?val1=$1&val2=$2&val3=$3 [L,QSA]
# Başına (?:js/)? ekleyerek hem ana dizini hem js klasörünü kapsıyoruz
RewriteRule ^(?:js/)?bytao-([a-zA-Z0-9]+)-([0-9]+)-([0-9]+)\.js$ script.php?val1=$1&val2=$2&val3=$3 [L,QSA]

RewriteRule ^QR.php$ cdn/qr.php [L]